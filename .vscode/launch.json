{
    "version": "0.2.0",
    "configurations": [{
            "name": "Launch server.js",
            "type": "node",
            "program": "${workspaceRoot}/server.js"
        },
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}